{
  "manifest_version": 3,
  "name": "WhyTab",
  "version": "1.3.0",
  "description": "Save, organize, and search your tabs. A smarter alternative to OneTab.",
  "permissions": [
    "tabs",
    "storage",
    "favicon",
    "downloads",
    "alarms"
  ],
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "icons/icon16.svg",
      "32": "icons/icon32.svg",
      "48": "icons/icon48.svg",
      "128": "icons/icon128.svg"
    }
  },
  "background": {
    "service_worker": "background/service-worker.js"
  },
  "options_page": "settings/settings.html",
  "icons": {
    "16": "icons/icon16.svg",
    "32": "icons/icon32.svg",
    "48": "icons/icon48.svg",
    "128": "icons/icon128.svg"
  }
}
